<UserControl x:Class="LetsQR.Log"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:LetsQR"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label HorizontalAlignment="Center"  Content="Logi" FontSize="36" Foreground="White" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <ListBox x:Name="logsList" ItemsSource="{Binding logs}" Foreground="White">

        </ListBox>
    </Grid>
</UserControl>
